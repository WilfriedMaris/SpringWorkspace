package be.vdab.entities;

public class Brouwer {
	private long brouwerNr;
	private String naam;
	private Integer omzet;
	
	public Brouwer(long brouwerNr, String naam, Integer omzet) {
		this.brouwerNr = brouwerNr;
		this.naam = naam;
		this.omzet = omzet;
	}

	public long getBrouwerNr() {
		return brouwerNr;
	}

	public void setBrouwerNr(long brouwerNr) {
		this.brouwerNr = brouwerNr;
	}

	public String getNaam() {
		return naam;
	}

	public void setNaam(String naam) {
		this.naam = naam;
	}

	public Integer getOmzet() {
		return omzet;
	}

	public void setOmzet(Integer omzet) {
		this.omzet = omzet;
	}

}
