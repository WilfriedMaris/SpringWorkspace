package be.vdab;
import java.util.List;

public class PersoonViewer {
	public void Afbeelden(List<Persoon> lijst){
		lijst.forEach(persoon -> System.out.println(
				persoon.getVoornaam + " " + 
				persoon.getFamilienaam + " " +
				persoon.getPersoonNr + " " + 
				persoon.getAantalKinderen));
	}
}
